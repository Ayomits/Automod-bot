{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@automod/bot",
  "description": "Discord.js implementation",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "format:fix": "npx prettier --write \"src/**/*.ts\"",
    "format:check": "npx prettier --check \"src/**/*.ts\"",
    "lint:check": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "build": "tsc -b && tsc-alias",
    "pipeline": "pnpm run format:fix && pnpm run lint:fix && pnpm run build",
    "dev": "npx nodemon",
    "prod": "node dist/main.js"
  },
  "repository": {
    "url": "https://github.com/Ayomits/ts-fetch"
  },
  "dependencies": {
    "@discordx/di": "^3.3.4",
    "@discordx/importer": "^1.3.3",
    "@discordx/utilities": "^7.0.5",
    "@ts-fetcher/cache": "^1.3.3",
    "@ts-fetcher/rest": "^1.3.5",
    "discord.js": "^14.21.0",
    "discordx": "^11.12.5",
    "dotenv": "^17.2.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@automod/types": "workspace:^",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@ts-fetcher/types": "^1.4.0",
    "@types/node": "^24.0.12",
    "@types/node-schedule": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "esbuild-plugin-version-injector": "^1.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "author": "Ayomits"
}
