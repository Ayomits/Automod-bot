{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@automod/automod",
  "description": "Discord.js implementation",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    "#*": ["./dist/*"]
  },
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint:check": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "build": "tsc -b",
    "pipeline": "pnpm run format:fix && pnpm run lint:fix && pnpm run build",
    "start:client:dev": "nodemon",
    "start:client:prod": "node dist/main.js"
  },
  "repository": {
    "url": "https://github.com/Ayomits/ts-fetch"
  },
  "dependencies": {
    "@automod/api": "workspace:^",
    "@automod/config": "workspace:^",
    "@discordx/di": "^3.3.4",
    "@discordx/importer": "^1.3.3",
    "@discordx/utilities": "^7.0.5",
    "@ts-fetcher/cache": "^1.4.1",
    "@ts-fetcher/rest": "^1.3.5",
    "discord.js": "^14.21.0",
    "discordx": "^11.12.5",
    "dotenv": "^17.2.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@automod/types": "workspace:^",
    "@ts-fetcher/types": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "esbuild-plugin-version-injector": "^1.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ],
  "author": "Ayomits"
}
